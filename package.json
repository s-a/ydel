{
  "name": "ydel",
  "version": "0.0.0",
  "description": "Global shell command to delete files and folder usings globs.",
  "homepage": "https://github.com/s-a/ydel",
  "author": {
    "name": "s-a",
    "email": "stephan.ahlf@googlemail.com",
    "url": "https://github.com/s-a"
  },
  "bin": {
